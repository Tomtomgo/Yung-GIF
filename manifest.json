{
  "manifest_version": 2,

  "name": "Yung GIF",
  "description": "Top 25 gifs on reddit - one click away.",
  "version": "0.1",
  "permissions": [
      "tabs",
      "storage",
      "alarms"
    ],
  "background": {
    "persistent": false,
    "scripts": ["jquery-2.1.0.min.js", "pingu.js", "yung_gif.js"]
  },
  "browser_action": {
    "default_icon": "crapicon.png",
    "tooltip": "Let's find GIFs."
  },
  "options_page": "i_want_more.html"
}